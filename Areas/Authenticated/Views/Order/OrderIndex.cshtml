@model List<WebApplication123.Models.OrderDetail>
@{
	Layout = "_Layout2";
}
<h1>Customer Ordered</h1>
<table class="table">
	<thead>
		<tr>
			<th>Id</th>
			<th>Customer Name</th>
			<th>Book Name</th>
			<th>Ordered Quantity</th>
			<th>Total Price</th>
			<th>Action</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>@item.OrderDetailId</td>
				<td>@item.Order.User.Name</td>
				<td>@item.Book.Name</td>
				<td>@item.Quantity</td>
				<td>Total</td>
				<td>Action</td>
			</tr>
		}
	</tbody>
</table>