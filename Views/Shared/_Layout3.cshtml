						<a href="#" class="flex-c-m trans-04 p-lr-25">
							USD
						</a>	
							
						
					</div>
				</div>
			</div>
			
			<div class="wrap-menu-desktop how-shadow1">
				<nav class="limiter-menu-desktop container mb-3">

					<!-- Logo desktop -->
					<a href="#" class="logo">
						<img src="~/Images/logo.png" alt="IMG-LOGO">
					</a>

					<!-- Menu desktop -->
					<div class="menu-desktop">
						<ul class="main-menu">
							<li>
								<a href="#">Home</a>
								
							</li>

							<li class="">
								<a asp-area="Authenticated" asp-controller="Book" asp-action="BookProduct">Shop</a>
								
							</li>

							<li class="label1" data-label1="hot">
								<a href="#">Features</a>
							</li>

							<li>
								<a href="blog.html">Blog</a>
							</li>

							<li>
								<a href="about.html">About</a>
							</li>

							<li>
								<a href="contact.html">Contact</a>
							</li>
						</ul>
					</div>

					<!-- Icon header -->
					<div class="wrap-icon-header flex-w flex-r-m">
						<div class="icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 js-show-modal-search">
							<i class="zmdi zmdi-search"></i>
						</div>

						<a asp-area="Authenticated" asp-controller="Book" asp-action="Cart" class="icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 icon-header-noti " >
							<i class="zmdi zmdi-shopping-cart"></i>
						</a>

						<a href="#" class="icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 icon-header-noti" data-notify="0">
							<i class="zmdi zmdi-favorite-outline"></i>
						</a>
					</div>
					<div class="navbar-expand-sm navbar-toggleable-sm navbar-light border-bottom pl-5">
						<div class="how-shadow1 rounded-pill">
							<div class="navbar-collapse collapse justify-content-between container">
								<partial name="_LoginPartial" />
							</div>
						</div>
					</div>
					
				</nav>
			</div>
		</div>
    </header>
    <div class="container">
        <main role="main" class="pb-3 p-t-135">
            @RenderBody()
        </main>
    </div>

